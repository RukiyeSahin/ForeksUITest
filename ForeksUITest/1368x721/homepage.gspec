      aligned vertically all takip-ettiklerim
      width 360px 
      height 62px
      below header 10 px
      above takip-ettiklerim 10 px 
      left-of favori-endeksler 10 px
      near takip-ettiklerim 10px top  
    takip-ettiklerim:
      inside body 82px top, 10px bottom, 998px right, 10px left
      aligned vertically all yatirimci-hesabim
      width 360px 
      height 460px
      below yatirimci-hesabim 10 px 
      left-of favori-endeksler 10 px 
      left-of haber-youtube 10 px 
      left-of bos 10 px
    teknik-analiz:
      inside body 10px top, 190px bottom, 10px right, 709px left
      aligned horizontally top yatirimci-hesabim
      width 649px 
      height 352px
      below header 10px 
      above ekonomik-takvim 9 px
      above bist-yukselenler 9 px
      right-of favori-endeksler 10 px 
      right-of haber-youtube 10 px 
      right-of bos 10 px
      near ekonomik-takvim 9px top 
      near bist-yukselenler 9px top 
    favori-endeksler:
      inside body 10px top, 371px bottom, 669px right, 380px left
      aligned horizontally top teknik-analiz
      aligned vertically all haber-youtube 
      width 319px 
      height 171px
      below header 10 px
      above haber-youtube 9 px
      left-of teknik-analiz 10px
      right-of takip-ettiklerim 10 px
      right-of yatirimci-hesabim 10 px
      near haber-youtube 9px top 
    haber-youtube:
      inside body 190px top, 191px bottom, 669px right, 380px left
