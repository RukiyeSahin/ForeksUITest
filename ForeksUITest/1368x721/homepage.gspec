      height 171px
      below favori-endeksler 9 px
      above bos 10px 
      right-of takip-ettiklerim 10 px
      left-of teknik-analiz 10 px
      near bos 10px top 
    bos:
      inside body 371px top, 10px bottom, 669px right, 380px left
      width 319px 
      height 171px
