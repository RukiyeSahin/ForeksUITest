= Header =
  @on *
    header:
      width 1368px 
      height 60px
      near body 0px top 
    header-left:
      inside header 14px top, 46px bottom, 10px right, 20px left
      left-of header-controls -893px
      width 1338px 
      height 0px
    header-right:
      inside header 14px top, 14px bottom, 10px right, 1172px left
      right-of header-controls 0px
      aligned horizontally top header-controls
      width 186px 
      height 32px
    header-controls:
      right-of header-left -893px
      left-of header-right 0px
      aligned horizontally all header-right
      width 707px 
      height 32px
    logo:
      left-of header-controls 321px
      width 124px 
      height 31px   
      image file ../image/logo2.png, error 141px 
    edit-list:
      inside header-controls 0px top, 0px bottom, 667px right, 0px left
      aligned horizontally all ekrantasarim-list
      right-of logo 321px
      left-of ekrantasarim-list 20px
      width 40px 
      height 32px 
    edit:
      inside edit-list 0px top, 0px bottom, 0px right, 0px left
      width 40px 
      height 32px 
    ekrantasarim-list:
      inside header-controls 0px top, 0px bottom, 367px right, 60px left
      aligned horizontally all modul-list
      right-of edit-list 20px
      left-of modul-list 20px
      width 280px 
      height 32px 
    hisselerim:
      inside ekrantasarim-list 0px top, 0px bottom, 90px right, 0px left
      width 190px 
      height 32px 
    save:
      inside ekrantasarim-list 0px top, 0px bottom, 45px right, 195px left
      width 40px 
      height 32px  
    save-as:
      inside ekrantasarim-list 0px top, 0px bottom, 0px right, 240px left
      width 40px 
      height 32px 
    modul-list:
      inside header-controls 0px top, 0px bottom,307px right, 360px left
      aligned horizontally all lisansal-list
      left-of lisansal-list 20px
      right-of ekrantasarim-list 20px
      width 40px 
      height 32px
    modul:
      inside modul-list 0px top, 0px bottom, 0px right, 0px left
      width 40px 
      height 32px 
    lisansal-list:
      inside header-controls 0px top, 0px bottom, 170px right, 420px left
      aligned horizontally all alsat-btn-list
      left-of alsat-btn-list 20px
      right-of modul-list 20px
      width 117px 
      height 32px
    lisansal-btn:
      width 117px 
      height 32px 
      css background-color is  "rgba(0, 0, 0, 0)"  
    alsat-btn-list:
      inside header-controls 0px top, 0px bottom, 65px right, 557px left
      aligned horizontally all alarm-list
      right-of lisansal-list 20px
      left-of alarm-list 20px
      width 85px 
      height 32px
    sat-btn:
      inside alsat-btn-list 0px top, 0px bottom, 0px right, 45px left
      width 40px 
      height 32px
      css background-color is  "rgba(220, 18, 18, 1)"  
    al-btn:
      inside alsat-btn-list 0px top, 0px bottom, 45px right, 0px left
      width 40px 
      height 32px 
      css background-color is  "rgba(97, 213, 0, 1)"  
    alarm-list:
      inside header-controls 0px top, 0px bottom, 5px right, 662px left
      aligned horizontally all saat-list
      left-of saat-list 15px
      right-of alsat-btn-list 20px
      width 40px 
      height 32px
    alarm:
      inside alarm-list 0px top, 0px bottom, 0px right, 0px left
      width 40px 
      height 32px 
    saat-list:
      aligned horizontally all profil-list
      left-of profil-list 10px
      right-of alarm-list 15px
      width 51px 
      height 32px
    profil-list: 
      aligned horizontally all cikis-list
      left-of cikis-list 20px
      right-of saat-list 10px  
      width 40px 
      height 32px
    cikis-list:
      inside header-right 0px top, 0px bottom, 0px right, 131px left
      right-of profil-list 20px
      width 55px 
      height 32px  
    cikis:
      inside cikis-list 0px top, 0px bottom, 0px right, 0px left
      css font-size is "14px" 
      css color is  "rgba(204, 166, 44, 1)"
      css background-color is  "rgba(242, 242, 242, 1)" 
= Body =
  @on *
    body:
      width 1368px 
      height 552px
      near header 0px bottom 
    yatirimci-hesabim:
      inside body 10px top, 480px bottom, 998px right, 10px left
